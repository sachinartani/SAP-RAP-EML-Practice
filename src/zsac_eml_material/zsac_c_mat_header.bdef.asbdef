projection;
strict ( 2 );
use draft;

define behavior for ZSAC_C_MAT_HEADER //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _MaterialPlant { create; with draft; }
}

define behavior for ZSAC_C_MAT_PLANT //alias <alias_name>
{
  use update;
  use delete;

  use association _MaterialHeader { with draft; }
}